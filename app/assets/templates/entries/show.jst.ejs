<div class="row entdiv" id="show_header">
	<div class="col-xs-3">IMG</div>
	<div class="col-xs-9">NAME</div>
</div>
<div class="row entdiv" id="show_title">
	<h2><%= entry.get("title") %></h2>
</div>
<div class="row" id="show_body">
	<h3>Dive Notes:</h3>
	<div class="interior_text_area">
		<%= entry.get("body") %>
	</div>
</div>
<div class="row" id="dive_base_info">
	<div class="col-xs-3">
		<h1><%= entry.get("divenum") %></h1>
	</div>
	<div class="col-xs-9">
		<ul>
			<li>Entry Date: <%= entry.get("entrydate") %></li>
			<li>Entry Time: <%= entry.get("entrytime") %></</li>
			<li>Location Name: <%= entry.get("location_name") %></</li>
			<li>Dive Type: <%= entry.get("divetype") %></</li>
		</ul>
	</div>
</div>
<div class="row entdiv">
	<h2>Conditions</h2>
</div>
<div class="row" id="entry_conditions">
	<div class="col-xs-9">
		<h3>Surface Conditions</h3>
		<ul>
			<li>Weather: <%= entry.get("weather") %></li>
			<li>Air Temp: <%= entry.get("airtemp") %></li>
			<li>Surface: <%= entry.get("surface") %></li>
		</ul>
	</div>
	<div class="col-xs-3">
		<h3>Underwater Conditions</h3>
		<ul>
			<li>Visibility: <%= entry.get("vis") %></li>
			<li>Current: <%= entry.get("current") %></li>
			<li>Water Temp: <%= entry.get("watertemp") %></li>
		</ul>
	</div>
</div>
<div class="row entdiv">
	<h2>Dive Profile</h2>
</div>
<div class="row">
	<div class="col-xs-4" id="max_depth_box">
		<h1><%= entry.get("maxdepth") %></h1>
	</div>
	<div class="col-xs-4" id="avg_depth_box">
		<h1><%= entry.get("avgdepth") %></h1>
	</div>
	<div class="col-xs-4" id="divetime_depth_box">
		<h1><%= entry.get("divetime") %></h1>
	</div>
</div>