<div class="row entdiv" id="show_header">
	<div class="col-xs-5">
		<h1 id="entry-title"><%= entry.escape("fname") %></h1>
	</div>
	<div class="col-xs-2">
			<% if (entry.user().has('image_url')) { %>
					<img id="entry-user-show-image" src="<%= entry.user().get('image_url') %>">
			<% } else { %>
					<img id="entry-user-show-image" src=<%= Diveloggr.defaultProfileImg %>>
			<% } %>
		</div>
	<div class="col-xs-5">
		<h1 id="entry-title"><%= entry.escape("lname") %></h1>
	</div>
</div>
